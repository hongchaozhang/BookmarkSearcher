{
  "name": "My Bookmarks",
  "version": "1.4",
  "description": "A browser action with a popup dump of searching all bookmarks, sort the results according to the number they match the input key words.",
  "permissions": [
    "bookmarks"
  ],
  "action": {
      "default_title": "My Bookmarks",
      "default_icon": "icon.png",
      "default_popup": "popup.html"
  },
  "icons": {
    "48": "icon.png"
  },
  "manifest_version": 3,
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Alt+B",
        "windows": "Alt+B",
        "mac": "Alt+B",
        "linux": "Alt+B"
      }
    }
  }
}